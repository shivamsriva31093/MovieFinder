    <declare-styleable name="GeneralTextView">
        <attr name="fontName" format="string" />
        <attr name="textStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italics" value="2" />/>
            <enum name="boldItalics" value="3" />
            <enum name="semiBold" value="5" />
            <enum name="extraBold" value="6" />
            <enum name="extraBoldItalics" value="7" />
            <enum name="light" value="8" />
            <enum name="medium" value="9" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTabLayout">
        <attr name="tabLayout" format="reference" />
        <attr name="tabFont" format="string" />
    </declare-styleable>

    <declare-styleable name="CollapsibleTextView">
        <attr name="trimLength" format="integer" />
    </declare-styleable>

    <item name="visible" type="id" />
    <item name="masked" type="id" />

</resources>
